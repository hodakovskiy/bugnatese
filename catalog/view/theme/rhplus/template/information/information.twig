{{ header }}
<div class="container">
  <ul class="breadcrumb">
    {% for breadcrumb in breadcrumbs %}
    <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
    {% endfor %}
  </ul>
  <div class="row">
    <div id="content" class="col-sm-12">
      {{ content_top }}
      <h1>{{ heading_title }}</h1>
      {{ description }}
      {{ content_bottom }}
    </div>
  </div>
</div>
{{ footer }}
